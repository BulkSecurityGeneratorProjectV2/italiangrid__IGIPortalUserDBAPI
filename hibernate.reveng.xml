<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<type-mapping>
		<sql-type jdbc-type="LONGVARCHAR" hibernate-type="string"
			length="65535" not-null="true">
		</sql-type>
		<sql-type jdbc-type="DATE" hibernate-type="date" length="10"
			not-null="true">
		</sql-type>
		<sql-type jdbc-type="CHAR" hibernate-type="string" length="5"
			not-null="true">
		</sql-type>
		<sql-type jdbc-type="INTEGER" hibernate-type="int"
			not-null="true">
		</sql-type>
		<sql-type jdbc-type="VARCHAR" hibernate-type="string"
			length="75" not-null="true">
		</sql-type>
	</type-mapping>
	<table-filter match-schema="PortalUser" match-name=".*"></table-filter>
	<table-filter match-name="IDP"></table-filter>
	<table-filter match-name="VO"></table-filter>
	<table-filter match-name="certificate"></table-filter>
	<table-filter match-name="userInfo"></table-filter>
	<table-filter match-name="userToVO"></table-filter>
	<table-filter match-name="notify"></table-filter>

	<table catalog="PortalUser" name="userToVO"></table>
	<table catalog="PortalUser" name="IDP"></table>
	<table catalog="PortalUser" name="certificate"></table>
	<table catalog="PortalUser" name="userInfo"></table>
	<table catalog="PortalUser" name="VO"></table>
	<table catalog="PortalUser" name="notify"></table>
</hibernate-reverse-engineering>